@model IEnumerable<UniversityManagementSystemMVCApp.Models.NetworkingClubTB>

@{
    ViewBag.Title = "Index";
}

<div class="row">
    <div class="col-6">
        <h2 class="text-info text-center">Member List</h2>
    </div>
</div>
<br />
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ExpDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Type)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StudentTB.StudentName)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ExpDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Type)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StudentTB.StudentName)
            </td>
        </tr>
    }

</table>